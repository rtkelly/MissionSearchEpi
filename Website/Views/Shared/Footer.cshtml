@using EPiServer.Editor
@using EPiServer.Core
@using EPiServer.Web
@using BaseSite.Models
@using BaseSite.Models.ViewModels
@using BaseSite.Models.Blocks


@model IPageViewModel<BasePage>


<footer class="footer">
    <div class="container">

    @if (Model.Layout.Footer.FooterMenu != null)
    {


        foreach (var menuItem in Model.Layout.Footer.FooterMenu.FilteredContents.OfType<FooterMenuBlock>())
        {
            Html.RenderPartial("FooterMenu", menuItem);

        }
    }

    </div>

</footer>